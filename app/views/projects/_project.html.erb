<div id="<%= dom_id project %>">
  <p>
    <strong>Title:</strong>
    <%= project.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= project.description %>
  </p>

  <p>
    <strong>Image url:</strong>
    <%= project.image_url %>
  </p>

  <p>
    <strong>Github url:</strong>
    <%= project.github_url %>
  </p>

  <p>
    <strong>Live url:</strong>
    <%= project.live_url %>
  </p>

</div>
